{
  "title": "⚡ ROUGEE CYBERDOCS ⚡",
  "description": ">> DECENTRALIZED MUSIC PLATFORM NEURAL INTERFACE << Access granted to digital music economy protocols",
  "author": "ROUGEE CyberTeam",
  "language": "en",
  "gitbook": "3.2.3",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "search",
    "sharing",
    "fontsettings",
    "highlight",
    "livereload",
    "lunr",
    "search-pro",
    "theme-default",
    "terminal",
    "prism",
    "emoji"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true,
      "styles": {
        "website": "styles/website.css"
      }
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook",
        "twitter"
      ]
    },
    "fontsettings": {
      "theme": "night",
      "family": "mono",
      "size": 2
    },
    "highlight": {
      "theme": "monokai"
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-material-dark.css"
      ]
    },
    "terminal": {
      "copyButtons": true,
      "fade": false,
      "style": "flat"
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Arial",
    "paperSize": "a4",
    "chapterMark": "pagebreak",
    "pageBreaksBefore": "/",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
}
